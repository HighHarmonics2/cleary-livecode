(1) +: (tile (step [4,2,1] (lfsaw (cps/4))) $ zoom (step [1, saw (cps/12)] (lfsaw cps)) $ spin (step [0, (time/4)] (lfsaw (cps/2))) $ setfxy [mod, ifft fy] $ (mod +: uv)) * [1,1,0.3] >> rgba;

