    -- # rel 1
    |+ n 24 

unsolo 3

d1 silence

all $ someCyclesBy 0.15 (linger "s 0.25" . ((# room (isaw)) . (# size (isaw))))

all $ id

d1
    $ every 2 (spreadf [hurry 0.5, jux (# verb 0.6 0.9 0.6 0.6 ), scramble 4, (# n "<1 7 7>") , off 0.125 (|* speed 1.5)])
    $ mask (binary 170)
    $ every 4 (jux rev)
    $ rarely (ply 2)
    $ loopAt 8
    $ chop 16 
    $ s "{pdbreaks:7}"
# shape 0.5
# verb 0.5 0.5 0.4 0.3
# cps 0.666
