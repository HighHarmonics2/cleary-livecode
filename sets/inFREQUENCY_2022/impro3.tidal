-- rhodes
d3
    -- $ plyWith 2 (0.25 <~)
    -- $ sometimes (# legato 0.3) 
    -- $ struct "t(<5 7>,8,<0 0 2 1>)"
    $ someCyclesBy 0.2 (mask "1" . ( arp (fast 2 $ "down converge")))
    $ (2 ~>)
    $ every 32 (struct "~@0.5 t@2 t@1.5" . (|+ note "2@1 0@3"))
    $ ("[0.75 ]"  ~>)
    $ slow 8
    $ n "<d4'min9 d5'min9'ii>"
    # s "[s100m808sinspace]"
    # legato "<1 0.23 1>"
