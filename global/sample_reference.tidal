-- Dirt-Samples
-- +pitched
-- +estuary
d1 $ slow 8 $ s "bass1" <| n (run 28) -- softer synth bass, 0 = C1, mostly C1 varying harshness, some F1
d1 $ slow 2 $ s "bass3" <| n (run 10) -- softer synth bass, 0 = A2, 1-4 = C#2, 5 = G#2, 6-7 = D3, 8 = C2 9-10 = B2(woah)
d1 $ slow 4 $ s "flbass" <| n (run 17) -- fretless bass, tuned to C, various attacks and releases 14/15=harmonics 
d1 $ slow 2 $ s "gtr" <| n (run 3) -- progressively more distorted guitar *=C3
d1 $ slow 2 $ s "juno" <| n (run 11) -- C1/3/5,ChC1/3/5,RelC1/3/5,Cm var 1/3/5
d1 $ slow 2 $ s "moog" <| n (run 7) -- C1,C2,C3,G0,G1,G2,G3
d1 $ slow 2 $ s "pluck" <| n (run 16) -- double bass, pitch in filename, 5=C3, vscode swaps # compared to superdirt
d1 $ slow 2 $ s "sawg" <| n (run 1) -- c4 beating synth sound
d1 $ slow 2 $ s "sax" <| n (run 21) -- sax long notes, pitch in filename, 5 = C2
d1 $ slow 2 $ s "sid" <| n (run 12) -- various synth sounds,1=~G1 3=C4 >7=perc
d1 $ slow 3 $ s "teclado" <| n(run 6) -- piano and synth, 0=C4 short release piano 1=C4 long rel piano 2-5 C4+ synths varying rel
d1 $ slow 6 $ s "volver" <| n(run 6) -- airtube sounds, 0=B3 1=? 2=F2 3=G2 4=? 5=B3 warble

